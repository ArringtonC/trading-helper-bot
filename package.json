  "scripts": {
    "start": "react-app-rewired start",
    "start:fast": "GENERATE_SOURCEMAP=false ESLINT_NO_DEV_ERRORS=true react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish",
    "electron-dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && ELECTRON_START_URL=http://localhost:3000 electron .\"",
    "electron-pack": "npm run build && electron-builder -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "npm run build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "start-server-and-test electron-dev http://localhost:3000 cypress:run",
    "dev": "concurrently -k -r \"npm:start\" \"wait-on http://localhost:3000 && ELECTRON_START_URL=http://localhost:3000 npx electron dist/electron.js\"",
    "update-trader-data": "npm run build && node scripts/automation/update-trader-data.ts",
    "test-sec-api": "npm run build && node -e \"const { secEdgarService } = require('./dist/services/SECEdgarService.js'); secEdgarService.testConnection().then(console.log);\"",
    "start-proxy": "node server/sec-edgar-proxy-server.js",
    "dev-with-proxy": "concurrently \"npm run start-proxy\" \"npm start\"",
    "dev-full": "node scripts/start-with-proxy.js"
  },
